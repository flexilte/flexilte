{
	"name": "@flexilte/skeleton",
	"version": "2.0.0",
	"description": "A skeleton support library for flexilte.",
	"keywords": [
		"components",
		"ui",
		"component",
		"svelte",
		"component-library",
		"ui-components",
		"ui-framework",
		"tailwind",
		"tailwindcss",
		"components-library",
		"sveltekit",
		"skeleton",
		"flexilte"
	],
	"homepage": "https://flexilte.github.io",
	"bugs": {
		"url": "https://github.com/flexilte/flexilte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/flexilte/flexilte.git"
	},
	"author": "borgmon <borgmon.contact@gmail.com>",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/lib/types.ts",
			"svelte": "./src/lib/index.ts",
			"default": "./src/lib/index.ts"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"./dist/**/*.svelte",
		"./dist/**/*.js",
		"./dist/**/*.d.ts",
		"./dist/**/*.cjs",
		"./dist/**/*.d.cts",
		"!./dist/**/*.test.*"
	],
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --write . && eslint --fix .",
		"lint": "prettier --check --write . && eslint .",
		"package": "svelte-kit sync && svelte-package",
		"prepublishOnly": "npm run package",
		"preview": "vite preview",
		"test": "vitest"
	},
	"dependencies": {
		"@flexilte/core": "workspace:^",
		"highlight.js": "11.11.1",
		"@iconify/svelte": "^5.0.1"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^3.2.0",
		"@skeletonlabs/skeleton-svelte": "^1.5.1",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@vitest/browser": "^3.2.3",
		"svelte": "^5.25.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"peerDependencies": {
		"svelte": "^5.25.0",
		"@skeletonlabs/skeleton": "^3.2.0"
	},
	"publishConfig": {
		"access": "public",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"svelte": "./dist/index.js",
				"default": "./dist/index.js"
			}
		},
		"types": "./dist/index.d.ts"
	},
	"links": {
		"documentation": "https://flexilte.github.io",
		"changelog": "https://github.com/flexilte/flexilte/blob/main/packages/skeleton/CHANGELOG.md"
	}
}
