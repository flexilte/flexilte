{
	"name": "@flexilte/skeleton",
	"version": "1.2.0",
	"description": "A skeleton support library for flexilte.",
	"keywords": [
		"components",
		"ui",
		"component",
		"svelte",
		"component-library",
		"ui-components",
		"ui-framework",
		"tailwind",
		"tailwindcss",
		"components-library",
		"sveltekit",
		"skeleton",
		"flexilte"
	],
	"homepage": "https://flexilte.github.io",
	"bugs": {
		"url": "https://github.com/flexilte/flexilte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/flexilte/flexilte.git"
	},
	"author": "borgmon <borgmon.contact@gmail.com>",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/lib/types.ts",
			"svelte": "./src/lib/index.ts",
			"default": "./src/lib/index.ts"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"./dist/**/*.svelte",
		"./dist/**/*.js",
		"./dist/**/*.d.ts",
		"./dist/**/*.cjs",
		"./dist/**/*.d.cts",
		"!./dist/**/*.test.*"
	],
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --write . && eslint --fix .",
		"lint": "prettier --check --write . && eslint --ignore-path ../../.gitignore .",
		"package": "svelte-kit sync && svelte-package",
		"prepublishOnly": "npm run package",
		"preview": "vite preview",
		"test": "vitest"
	},
	"dependencies": {
		"highlight.js": "11.9.0"
	},
	"devDependencies": {
		"@iconify/svelte": "^4.0.2",
		"@skeletonlabs/skeleton": "2.10.0",
		"@skeletonlabs/tw-plugin": "0.4.0",
		"@sveltejs/adapter-auto": "^3.2.1",
		"@sveltejs/kit": "^2.5.10",
		"@sveltejs/package": "^2.3.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/eslint": "^8.56.10",
		"@types/node": "20.12.12",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@typescript-eslint/parser": "^7.11.0",
		"autoprefixer": "10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.0",
		"postcss": "8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"publint": "^0.1.16",
		"svelte": "^4.2.17",
		"svelte-check": "^3.7.1",
		"tailwindcss": "3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.12",
		"vite-plugin-tailwind-purgecss": "0.3.3",
		"vitest": "^1.6.0"
	},
	"peerDependencies": {
		"@skeletonlabs/skeleton": "^2.10.0",
		"svelte": "^4.0.0"
	},
	"publishConfig": {
		"access": "public",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"svelte": "./dist/index.js",
				"default": "./dist/index.js"
			}
		},
		"types": "./dist/index.d.ts"
	},
	"links": {
		"documentation": "https://flexilte.github.io",
		"changelog": "https://github.com/flexilte/flexilte/blob/main/packages/skeleton/CHANGELOG.md"
	}
}
