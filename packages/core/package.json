{
	"name": "@flexilte/core",
	"version": "3.1.0",
	"description": "A SvelteKit layout library.",
	"keywords": [
		"components",
		"ui",
		"component",
		"svelte",
		"component-library",
		"ui-components",
		"ui-framework",
		"tailwind",
		"tailwindcss",
		"components-library",
		"sveltekit"
	],
	"homepage": "https://flexilte.github.io",
	"bugs": {
		"url": "https://github.com/flexilte/flexilte/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/flexilte/flexilte.git"
	},
	"author": "borgmon <borgmon.contact@gmail.com>",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/lib/index.ts",
			"svelte": "./src/lib/index.ts"
		}
	},
	"files": [
		"./dist/**/*.svelte",
		"./dist/**/*.js",
		"./dist/**/*.d.ts",
		"./dist/**/*.cjs",
		"./dist/**/*.d.cts",
		"!./dist/**/*.test.*"
	],
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --write . && eslint --fix .",
		"lint": "prettier --check --write . && eslint .",
		"package": "svelte-kit sync && svelte-package",
		"prepublishOnly": "npm run package",
		"preview": "vite preview",
		"test": "vitest"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@vitest/browser": "^3.2.3",
		"svelte": "^5.25.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"peerDependencies": {
		"svelte": "^5.25.0"
	},
	"publishConfig": {
		"access": "public",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"svelte": "./dist/index.js"
			}
		}
	},
	"links": {
		"documentation": "https://flexilte.github.io",
		"changelog": "https://github.com/flexilte/flexilte/blob/main/packages/core/CHANGELOG.md"
	}
}
